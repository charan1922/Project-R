{
  "name": "sensibull-extractor",
  "version": "2.0.0",
  "description": "Complete Sensibull Verified P&L Extractor with Web Dashboard",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "extract": "pnpm ts-node --project src/tsconfig.json src/extract.ts",
    "extract:v2": "pnpm ts-node --project src/tsconfig.json src/extract_v2.ts",
    "mcp": "pnpm ts-node --project src/tsconfig.json src/mcp-server.ts",
    "dev": "next dev -p 5000",
    "build": "next build",
    "start": "next start -p 5000",
    "serve": "pnpm serve dist -p 5000",
    "lint": "eslint",
    "install:browsers": "pnpm exec playwright install chromium",
    "clean": "rm -rf node_modules dist .next pnpm-lock.yaml"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@playwright/test": "^1.58.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.564.0",
    "mathjs": "^15.1.1",
    "next": "16.1.6",
    "playwright": "^1.58.2",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "stock-nse-india": "^1.3.0",
    "tailwind-merge": "^3.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "serve": "^14.2.5",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
